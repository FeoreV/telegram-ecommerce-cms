{
  "name": "telegram-ecommerce-backend",
  "version": "1.0.0",
  "description": "Backend API for Telegram ecommerce bot",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "cross-env ADMIN_JS_SKIP_BUNDLE=true tsx watch src/index.ts",
    "dev:admin": "cross-env ENABLE_ADMINJS=true ADMIN_JS_SKIP_BUNDLE=true tsx watch src/index.ts",
    "dev:noadmin": "cross-env ENABLE_ADMINJS=false tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "db:pull": "npx prisma db pull",
    "vault:setup": "bash ../tools/vault/setup-vault-secrets.sh",
    "vault:dev": "docker-compose -f ../config/vault/docker-compose.vault.yml up -d"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/prisma": "^5.0.4",
    "@adminjs/upload": "^4.0.2",
    "@prisma/client": "^5.6.0",
    "@types/node-fetch": "^2.6.13",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.24",
    "@types/speakeasy": "^2.0.10",
    "adminjs": "^7.8.17",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-formidable": "^1.2.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.0.1",
    "file-type": "^21.0.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.0",
    "isomorphic-dompurify": "^2.28.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^6.9.7",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "redis": "^4.6.10",
    "sharp": "^0.34.4",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.10",
    "@types/node-telegram-bot-api": "^0.64.11",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.0",
    "@types/validator": "^13.15.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "patch-package": "^8.0.0",
    "prisma": "^5.6.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
